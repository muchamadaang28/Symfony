{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ContactUs</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Nohp</th>
                <td>{{ entity.nohp }}</td>
            </tr>
            <tr>
                <th>Companyname</th>
                <td>{{ entity.companyName }}</td>
            </tr>
            <tr>
                <th>Judul</th>
                <td>{{ entity.judul }}</td>
            </tr>
            <tr>
                <th>Isipesan</th>
                <td>{{ entity.isiPesan }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('contactus') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('contactus_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
